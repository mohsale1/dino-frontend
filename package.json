  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "build:dev": "craco build",
    "build:analyze": "npm run build && npx serve -s build",
    "eject": "react-scripts eject",
    "deploy": "./deployment/scripts/deploy.sh production",
    "deploy:dev": "./deployment/scripts/deploy.sh development",
    "deploy:staging": "./deployment/scripts/deploy.sh staging",
    "deploy:build": "./deployment/scripts/build.sh && ./deployment/scripts/deploy.sh production",
    "deploy:quick": "./deployment/scripts/quick-deploy.sh",
    "clean": "rm -rf build node_modules/.cache",
    "build:prod": "./deployment/scripts/build.sh",
    "validate:env": "./deployment/scripts/validate-env.sh",
    "config:generate": "./deployment/scripts/generate-config.sh",
    "serve": "npx serve -s build -l 3000",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
