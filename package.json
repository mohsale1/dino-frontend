    "ajv-keywords": "^3.5.2",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "https-browserify": "^1.0.0",
    "node-fetch": "^2.7.0",
    "process": "^0.11.10",
